generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model clientes {
  nombre  String @db.VarChar
  celular String @db.VarChar
  id      String @id @db.VarChar
}

model productos {
  nombre String? @default("") @db.VarChar
  precio Float?  @default(0) @db.Real
  url    String? @default("")
  id     String  @id @db.VarChar
}
